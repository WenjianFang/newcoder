public class PalindromeList {
    public boolean chkPalindrome(ListNode A) {
    	// write code here
    	ListNode list1 = A;
    	ListNode list2 = A;
    	divideListAndRiverse(list1, list2);
    }
    private void divideListAndRiverse(ListNode list1, ListNode list2){
    	ListNode tmp = list1;
    	int count = 0;
    	while(tmp != null){
    		count++;
    		tmp = tmp.next;
    	}
    	
    }
}