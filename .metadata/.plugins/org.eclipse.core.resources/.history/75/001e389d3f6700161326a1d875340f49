public class PalindromeList {
    public boolean chkPalindrome(ListNode A) {
    	// write code here
    	ListNode list1 = A;
    	ListNode list2 = A;
    	int n = divideList(list1, list2);
    	reverse(list1, n);
    }
    private void reverse(ListNode list1){
    	
    }
    private int divideList(ListNode list1, ListNode list2){
    	ListNode tmp = list1;
    	int count = 0;
    	while(tmp != null){
    		count++;
    		tmp = tmp.next;
    	}
    	tmp = list1;
    	int i = count/2;
    	ListNode p = tmp;
    	while(i > 0){
    		p = tmp;
    		tmp = tmp.next;
    		i--;
    	}
    	p.next = null;
    	if(count % 2 == 0) list2 = tmp;
    	else list2 = tmp.next;
    	
    	return count/2;
    }
}