import java.util.Arrays;


public class Main {

	public static void main(String[] args){
		int[] stack = {1,2,3,4,5};
		int top = 5;
		
		System.out.println(Arrays.toString(stack));
		int[] ret = new ReverseStack().reverseStackRecursively(stack, top);
		System.out.println(Arrays.toString(ret));
	}
	
}
